{
  "name": "bed-roses",
  "version": "1.0.0",
  "scripts": {
    "database": "json-server ./api/database.json",
    "server:build": "BABEL_ENV=server babel ./index.js --out-dir ./dist",
    "server:start": "nodemon ./dist/index.js",
    "client": "NODE_ENV=development webpack --config webpack.config.client.js",
    "start": "npm-run-all --parallel server:* client"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.0-beta.6",
    "babel-plugin-css-modules-transform": "^1.6.1",
    "css-loader": "^1.0.0",
    "express": "^4.16.4",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.4",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-node-externals": "^1.7.2"
  },
  "license": "MIT"
}
